<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ca.afontaine.imageprocessor.app" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="ca.afontaine.imageprocessor.app.ImageSelector"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="ca.afontaine.imageprocessor.app.ImageProcessor"
            android:label="@string/title_activity_image_processor"
            android:parentActivityName="ca.afontaine.imageprocessor.app.ImageSelector" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ca.afontaine.imageprocessor.app.ImageSelector" />
        </activity>
    </application>

</manifest>
